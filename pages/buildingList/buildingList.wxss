.house-condition-box {
  height: 100%;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}
.house-condition-title {
  height: 88rpx;
  padding-left: 15rpx;
  line-height: 88rpx;
  font-size: 32rpx;
  font-weight: bold;
  box-sizing: border-box;
}
.value-class {
  flex: none !important;
}
.house-condition {
  flex: 1;
  overflow-y: auto;
  padding-bottom: 10rpx;
}
.check-box {
  padding: 15rpx 60rpx;
}
.input-box {
  padding: 15rpx 60rpx;
}
.house-condition-check{
  height: 100%;
  width: 60%; 
}
.house-condition-btn {
  height: 100rpx;
  display: flex;
}
.confirm-btn{
  width: 50%;
  padding-top: 12rpx;
  padding-left: 10rpx;
  padding-right: 20rpx;
}
.reset-btn{
  width: 50%;
  padding-top: 12rpx;
  padding-left: 10rpx;
  padding-right: 20rpx;
}

